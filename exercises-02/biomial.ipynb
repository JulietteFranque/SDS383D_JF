   "source": [
    "p = 0.7\n",
    "n = 10\n",
    "x = np.linspace(0,1,300)\n",
    "rv = binom(n, p)\n",
    "x = np.arange(rv.ppf(0.01),\n",
    "              rv.ppf(0.99))\n",
    "plt.plot(x, rv.pmf(x))"
